<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
	<item android:state_enabled="false">
		<shape >
			<corners android:radius="5dp"/>
			<stroke android:width="0.5dp"  android:color="#888"/>
		</shape>
	</item>
	<item android:state_focused="true">
		<shape >
			<corners android:radius="5dp"/>
			<gradient android:angle="90" android:startColor="#fff" android:centerColor="#fff"
					  android:endColor="#fff"/>
			<stroke android:width="0.5dp"  android:color="#65e"/>
		</shape>

	</item>
	<item >
		<shape >
			<corners android:radius="5dp"/>
			<gradient android:angle="90" android:startColor="#fff" android:centerColor="#fff"
					  android:endColor="#fff"/>
			<stroke android:width="0.5dp"  android:color="#8bbb"/>
		</shape>

	</item>
</selector>